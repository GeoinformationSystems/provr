@base <localhost://> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rscript: <https://www.idiv.de/de/luckinet.html/geokur/#> .

rscript:allocate
    a prov:Activity ;
    rdfs:label "allocate" ;
    prov:used rscript:lu_max_c, rscript:lu_min_c, rscript:mp_suit, rscript:suit_max, rscript:suit_min .

rscript:allocated_data
    a prov:Entity ;
    rdfs:label "allocated data" ;
    prov:wasGeneratedBy rscript:allocate .

rscript:census_areas
    a prov:Entity ;
    rdfs:label "census area" .

rscript:determine_correction_factors
    a prov:Activity ;
    rdfs:comment "correct limits based on supply/demand ratio" ;
    rdfs:label "determine correction factors" ;
    prov:used rscript:census_areas, rscript:lc_limits, rscript:mp_ahIDs, rscript:suitability_weighted_supply_map .

rscript:determine_landuse_limits
    a prov:Activity ;
    rdfs:label "determine land use limits" ;
    prov:used rscript:lc_limits, rscript:mp_cover .

rscript:determine_suitability_limits
    a prov:Activity ;
    rdfs:label "determine suitability limits" ;
    prov:used rscript:lc_limits, rscript:mp_ahIDs, rscript:mp_suit .

rscript:lc_limits
    a prov:Entity ;
    rdfs:label "land cover limits" .

rscript:lu_max
    a prov:Entity ;
    rdfs:label "land use maximum" ;
    prov:wasGeneratedBy rscript:determine_landuse_limits .

rscript:lu_max_c
    a prov:Entity ;
    rdfs:label "land use maximum corrected" ;
    prov:wasGeneratedBy rscript:determine_correction_factors .

rscript:lu_min
    a prov:Entity ;
    rdfs:label "land use minimum" ;
    prov:wasGeneratedBy rscript:determine_landuse_limits .

rscript:lu_min_c
    a prov:Entity ;
    rdfs:label "land use minimum corrected" ;
    prov:wasGeneratedBy rscript:determine_correction_factors .

rscript:mp_ahIDs
    a prov:Entity ;
    rdfs:label "administrive areas" .

rscript:mp_cover
    a prov:Entity ;
    rdfs:label "land cover map" .

rscript:mp_suit
    a prov:Entity ;
    rdfs:label "suitability map" .

rscript:pre_alloc
    a prov:Activity ;
    rdfs:comment "min-max normalisation" ;
    rdfs:label "pre allocate" ;
    prov:used rscript:lu_max, rscript:lu_min, rscript:suit_max, rscript:suit_min .

rscript:suit_max
    a prov:Entity ;
    rdfs:label "suitability maximum" ;
    prov:wasGeneratedBy rscript:determine_suitability_limits .

rscript:suit_min
    a prov:Entity ;
    rdfs:label "suitability minimum" ;
    prov:wasGeneratedBy rscript:determine_suitability_limits .

rscript:suitability_weighted_supply_map
    a prov:Entity ;
    rdfs:label "suiatability weighted supply map" ;
    prov:wasGeneratedBy rscript:pre_alloc .

